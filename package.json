{
  "name": "jira-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "git pull && npm-run-all build server",
    "startdev": "git pull && npm-run-all build serverdev",
    "dev": "nodemon --watch src/ --exec yarn startdev",
    "server": "touch server.log && fexport LD_LIBRARY_PATH=/opt/oracle/instantclient_19_6 && cp -rf ./public ./dist-server/public && forever start -a -o server.log -e server.log ./dist-server/app.js && tail -f server.log",
    "serverdev": "export LD_LIBRARY_PATH=/opt/oracle/instantclient_19_6 && cp -rf ./public ./dist-server/public && node ./dist-server/app",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "test": "mocha --require @babel/register \"src/**/*.js\" --reporter spec --exit",
    "cover": "./node_modules/.bin/nyc --reporter=html mocha --require @babel/register \"src/**/*.spec.js\" --exit"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "activedirectory2": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "forever": "^3.0.4",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "js-base64": "^3.3.2",
    "json2csv": "^5.0.1",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.31",
    "morgan": "~1.9.1",
    "mysql": "~2.16.0",
    "node-fetch": "^2.6.1",
    "oracledb": "^4.2.0",
    "query-string": "^6.12.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2",
    "safe-flat": "^1.1.2",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.1.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
